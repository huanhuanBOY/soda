}

.five {
  /* Chrome, Safari, Opera */ 
  -webkit-animation-duration:5.5s; 
  -webkit-animation-delay: .3s;
   /* Standard*/ 
  animation-duration:5.5s; 
  animation-delay: .3s;
}

.six {
  /* Chrome, Safari, Opera */ 
  -webkit-animation-duration: 5.5s;  
  -webkit-animation-delay: .35s;
   /* Standard*/ 
  animation-duration: 5.5s; 
  animation-delay: .35s;
}

.seven {
  /* Chrome, Safari, Opera */ 
  -webkit-animation-duration: 5.5s; 
  -webkit-animation-delay: .4s;
   /* Standard*/ 
  animation-duration: 5.5s; 
  animation-delay: .4s;
}

.eight {
  /* Chrome, Safari, Opera */ 
  -webkit-animation-duration: 5.5s; 
  -webkit-animation-delay: .45s;
   /* Standard*/ 
  animation-duration: 5.5s; 
  animation-delay: .45s;
}

.nine {
  /* Chrome, Safari, Opera */ 
  -webkit-animation-duration: 5.5s; 
    -webkit-animation-delay: .5s;
   /* Standard*/ 
  animation-duration: 5.5s; 
  animation-delay: .5s;
}

